<h1>Sign Up</h1>
<%= form_tag sessions_index_path, method: :post do %>
	<%= label_tag('Password') %>
	<%= text_field_tag :password, params[:password] %>
    <br />
    <br />
    <%= label_tag('Confirm Password') %>
	<%= text_field_tag :password, params[:password] %>
    <br />
    <br />
	<%= submit_tag "Sign Up" %>
<% end %>
